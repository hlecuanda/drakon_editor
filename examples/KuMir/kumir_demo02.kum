алг фибоначчи(цел число, рез цел таб фиб[0:1000])
    дано число <= 1000 и число >= 0
нач
    цел i
    если число = 0 то
        фиб[0] := 0
    иначе
        если число = 1 то
            фиб[0] := 0
            фиб[1] := 1
        иначе
            фиб[0] := 0
            фиб[1] := 1
            i := 2;
            нц
                если i <= число то
                    
                иначе
                    выход
                все
                фиб[i] := фиб[i-1] + фиб[i-2]
                i := i + 1;
            кц
        все
    все
кон

алг циклы со стрелкой
нач
    цел таб числа[1:20]
    цел счётчик
    счётчик := 1
    нц
        числа[счётчик] := счётчик * 3
        счётчик := счётчик + 1
        если счётчик > 20 то
            выход
        все
    кц
    счётчик := 1
    нц
        если счётчик < 20 то
            
        иначе
            выход
        все
        вывод числа[счётчик], " "
        счётчик := счётчик + 2
    кц
    вывод нс
    счётчик := 1
    нц
        счётчик := счётчик + 3
        если счётчик > 20 то
            выход
        все
        вывод числа[счётчик], " "
    кц
    вывод нс
кон

